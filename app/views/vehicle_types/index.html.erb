<p id="notice"><%= notice %></p>

<h1>Vehicle Types</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Subtype</th>
      <th>Year</th>
      <th>Tax</th>
      <th>Prime</th>
      <th>Fosyga</th>
      <th>Runt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_types.each do |vehicle_type| %>
      <tr>
        <td><%= vehicle_type.code %></td>
        <td><%= vehicle_type.name %></td>
        <td><%= vehicle_type.subtype %></td>
        <td><%= vehicle_type.year %></td>
        <td><%= vehicle_type.tax %></td>
        <td><%= vehicle_type.prime %></td>
        <td><%= vehicle_type.fosyga %></td>
        <td><%= vehicle_type.runt %></td>
        <td><%= link_to 'Show', vehicle_type %></td>
        <td><%= link_to 'Edit', edit_vehicle_type_path(vehicle_type) %></td>
        <td><%= link_to 'Destroy', vehicle_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle Type', new_vehicle_type_path %>
